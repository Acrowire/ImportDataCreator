                    <Separator Width="15" Background="{x:Null}"/>
                    <TextBox 
                             Text="{Binding ProNetScanResult.IntegrationStatus.Value2, Mode=TwoWay}" 
                             Width="100"/>
                    <TextBox TextAlignment="Right"
                             Text="{Binding ProNetScanResult.IntegrationStatus.Amount2, Mode=TwoWay}"
                             Width="50"/>
                    <ComboBox 
                              SelectedValue="{Binding ProNetScanResult.IntegrationStatus.Option2}" 
