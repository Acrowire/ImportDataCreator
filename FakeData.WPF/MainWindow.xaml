<Window x:Class="FakeData.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FakeData.WPF"
        mc:Ignorable="d"
        xmlns:vm="clr-namespace:FakeData.WPF.ViewModels"
        xmlns:helpers="clr-namespace:FakeData.WPF.ViewModels.Helpers"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Title="Fake Data" Height="550" Width="700" >
    <Window.DataContext>
        <vm:MainWindowVM/>
    </Window.DataContext>
    <Window.Resources>
        <helpers:BoolToStringConverter x:Key="BooleanToStringConverter" FalseValue="No" TrueValue="Yes"/>
    </Window.Resources>
    <Grid >
        <ScrollViewer>
            <StackPanel Orientation="Vertical" >
                <StackPanel Orientation="Horizontal" Height="30">
                    <Label Width="170">Number of rows to generate:</Label>
                    <TextBox Name="tbRows" Text="{Binding AmountRows, Mode=TwoWay}" Width="50" Height="25"/>
                    <Separator Width="10" Background="{x:Null}" />
                    <Button Content="Insert Records" Command="{Binding SaveCommand}" Height="25"/>
                </StackPanel>
                <Separator Height="10" Background="{x:Null}"/>
